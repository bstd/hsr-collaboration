include ../partials/header.jade

section(layout='row', flex)
  h1
    | Suchresultate: {{query}}
md-content(layout='row', layout-wrap, class='md-padding')
  md-card(ng-repeat='result in results | filter:query', flex='25', flex-md='33', flex-sm='100')
    md-card-content
      p
        | {{result.name}}
      p
        | {{result.price}}
      p
        | {{result.info}}
